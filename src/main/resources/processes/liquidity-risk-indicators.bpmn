<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL"
                   xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                   xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                   xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                   xmlns:drools="http://www.jboss.org/drools"
                   xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL BPMN20.xsd"
                   id="Definitions_1"
                   targetNamespace="http://www.jboss.org/drools">

  <bpmn2:itemDefinition id="_reportItem" structureRef="com.wahda.liquidity.model.LiquidityReport"/>
  <bpmn2:itemDefinition id="_stringItem" structureRef="String"/>
  <bpmn2:itemDefinition id="_integerItem" structureRef="Integer"/>
  <bpmn2:itemDefinition id="_booleanItem" structureRef="Boolean"/>

  <bpmn2:process id="LiquidityRiskIndicators"
                 name="Liquidity Risk Indicators Process"
                 isExecutable="true"
                 processType="Private">

    <!-- Process Variables -->
    <bpmn2:property id="report" itemSubjectRef="_reportItem" name="report"/>
    <bpmn2:property id="selectedYear" itemSubjectRef="_integerItem" name="selectedYear"/>
    <bpmn2:property id="selectedQuarterRange" itemSubjectRef="_stringItem" name="selectedQuarterRange"/>
    <bpmn2:property id="employeeComment" itemSubjectRef="_stringItem" name="employeeComment"/>
    <bpmn2:property id="managerComment" itemSubjectRef="_stringItem" name="managerComment"/>
    <bpmn2:property id="managerDecision" itemSubjectRef="_stringItem" name="managerDecision"/>
    <bpmn2:property id="directorComment" itemSubjectRef="_stringItem" name="directorComment"/>
    <bpmn2:property id="errorMessage" itemSubjectRef="_stringItem" name="errorMessage"/>
    <bpmn2:property id="success" itemSubjectRef="_booleanItem" name="success"/>

    <!-- Start Event -->
    <bpmn2:startEvent id="StartEvent_1" name="Start">
      <bpmn2:outgoing>SequenceFlow_1</bpmn2:outgoing>
    </bpmn2:startEvent>

    <!-- Screen 1: Period Selection -->
    <bpmn2:userTask id="UserTask_PeriodSelection" name="Screen 1: Period Selection">
      <bpmn2:extensionElements>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("Screen 1: Period Selection started");]]></drools:script>
        </drools:onEntry-script>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
      <bpmn2:incoming>SequenceFlow_Return</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
      <bpmn2:ioSpecification id="InputOutputSpecification_1">
        <bpmn2:dataInput id="DataInput_1" name="TaskName"/>
        <bpmn2:dataOutput id="DataOutput_1" itemSubjectRef="_integerItem" name="selectedYear"/>
        <bpmn2:dataOutput id="DataOutput_2" itemSubjectRef="_stringItem" name="selectedQuarterRange"/>
        <bpmn2:inputSet id="InputSet_1">
          <bpmn2:dataInputRefs>DataInput_1</bpmn2:dataInputRefs>
        </bpmn2:inputSet>
        <bpmn2:outputSet id="OutputSet_1">
          <bpmn2:dataOutputRefs>DataOutput_1</bpmn2:dataOutputRefs>
          <bpmn2:dataOutputRefs>DataOutput_2</bpmn2:dataOutputRefs>
        </bpmn2:outputSet>
      </bpmn2:ioSpecification>
      <bpmn2:dataInputAssociation id="DataInputAssociation_1">
        <bpmn2:targetRef>DataInput_1</bpmn2:targetRef>
        <bpmn2:assignment id="Assignment_1">
          <bpmn2:from xsi:type="bpmn2:tFormalExpression">Screen1_PeriodSelection</bpmn2:from>
          <bpmn2:to xsi:type="bpmn2:tFormalExpression">DataInput_1</bpmn2:to>
        </bpmn2:assignment>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataOutputAssociation id="DataOutputAssociation_1">
        <bpmn2:sourceRef>DataOutput_1</bpmn2:sourceRef>
        <bpmn2:targetRef>selectedYear</bpmn2:targetRef>
      </bpmn2:dataOutputAssociation>
      <bpmn2:dataOutputAssociation id="DataOutputAssociation_2">
        <bpmn2:sourceRef>DataOutput_2</bpmn2:sourceRef>
        <bpmn2:targetRef>selectedQuarterRange</bpmn2:targetRef>
      </bpmn2:dataOutputAssociation>
      <bpmn2:potentialOwner id="PotentialOwner_1">
        <bpmn2:resourceAssignmentExpression id="ResourceAssignmentExpression_1">
          <bpmn2:formalExpression>risk-employee</bpmn2:formalExpression>
        </bpmn2:resourceAssignmentExpression>
      </bpmn2:potentialOwner>
    </bpmn2:userTask>

    <!-- Service Task: Retrieve Data -->
    <bpmn2:task id="Task_RetrieveData" name="Retrieve Data from SRS" drools:taskName="DataRetrieval">
      <bpmn2:incoming>SequenceFlow_2</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_3</bpmn2:outgoing>
      <bpmn2:ioSpecification id="InputOutputSpecification_2">
        <bpmn2:dataInput id="DataInput_2" itemSubjectRef="_reportItem" name="report"/>
        <bpmn2:dataOutput id="DataOutput_3" itemSubjectRef="_booleanItem" name="success"/>
        <bpmn2:inputSet id="InputSet_2">
          <bpmn2:dataInputRefs>DataInput_2</bpmn2:dataInputRefs>
        </bpmn2:inputSet>
        <bpmn2:outputSet id="OutputSet_2">
          <bpmn2:dataOutputRefs>DataOutput_3</bpmn2:dataOutputRefs>
        </bpmn2:outputSet>
      </bpmn2:ioSpecification>
      <bpmn2:dataInputAssociation id="DataInputAssociation_2">
        <bpmn2:sourceRef>report</bpmn2:sourceRef>
        <bpmn2:targetRef>DataInput_2</bpmn2:targetRef>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataOutputAssociation id="DataOutputAssociation_3">
        <bpmn2:sourceRef>DataOutput_3</bpmn2:sourceRef>
        <bpmn2:targetRef>success</bpmn2:targetRef>
      </bpmn2:dataOutputAssociation>
    </bpmn2:task>

    <!-- Gateway: Check Data -->
    <bpmn2:exclusiveGateway id="Gateway_DataCheck" name="Data Found?" gatewayDirection="Diverging">
      <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_DataOk</bpmn2:outgoing>
      <bpmn2:outgoing>SequenceFlow_DataError</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>

    <!-- Error End -->
    <bpmn2:endEvent id="EndEvent_Error" name="Error End">
      <bpmn2:incoming>SequenceFlow_DataError</bpmn2:incoming>
      <bpmn2:errorEventDefinition id="ErrorEventDefinition_1" errorRef="Error_1"/>
    </bpmn2:endEvent>

    <!-- Screen 2: Indicators Display -->
    <bpmn2:userTask id="UserTask_IndicatorsDisplay" name="Screen 2: Indicators Display">
      <bpmn2:incoming>SequenceFlow_DataOk</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing>
      <bpmn2:ioSpecification id="InputOutputSpecification_3">
        <bpmn2:dataInput id="DataInput_3" itemSubjectRef="_reportItem" name="report"/>
        <bpmn2:dataInput id="DataInput_4" name="TaskName"/>
        <bpmn2:dataOutput id="DataOutput_4" itemSubjectRef="_stringItem" name="employeeComment"/>
        <bpmn2:inputSet id="InputSet_3">
          <bpmn2:dataInputRefs>DataInput_3</bpmn2:dataInputRefs>
          <bpmn2:dataInputRefs>DataInput_4</bpmn2:dataInputRefs>
        </bpmn2:inputSet>
        <bpmn2:outputSet id="OutputSet_3">
          <bpmn2:dataOutputRefs>DataOutput_4</bpmn2:dataOutputRefs>
        </bpmn2:outputSet>
      </bpmn2:ioSpecification>
      <bpmn2:dataInputAssociation id="DataInputAssociation_3">
        <bpmn2:sourceRef>report</bpmn2:sourceRef>
        <bpmn2:targetRef>DataInput_3</bpmn2:targetRef>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataInputAssociation id="DataInputAssociation_4">
        <bpmn2:targetRef>DataInput_4</bpmn2:targetRef>
        <bpmn2:assignment id="Assignment_2">
          <bpmn2:from xsi:type="bpmn2:tFormalExpression">Screen2_IndicatorsDisplay</bpmn2:from>
          <bpmn2:to xsi:type="bpmn2:tFormalExpression">DataInput_4</bpmn2:to>
        </bpmn2:assignment>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataOutputAssociation id="DataOutputAssociation_4">
        <bpmn2:sourceRef>DataOutput_4</bpmn2:sourceRef>
        <bpmn2:targetRef>employeeComment</bpmn2:targetRef>
      </bpmn2:dataOutputAssociation>
      <bpmn2:potentialOwner id="PotentialOwner_2">
        <bpmn2:resourceAssignmentExpression id="ResourceAssignmentExpression_2">
          <bpmn2:formalExpression>risk-employee</bpmn2:formalExpression>
        </bpmn2:resourceAssignmentExpression>
      </bpmn2:potentialOwner>
    </bpmn2:userTask>

    <!-- Script: Update Status -->
    <bpmn2:scriptTask id="ScriptTask_UpdateStatus1" name="Update Status" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_5</bpmn2:outgoing>
      <bpmn2:script><![CDATA[System.out.println("Status updated to SUBMITTED_TO_MANAGER");]]></bpmn2:script>
    </bpmn2:scriptTask>

    <!-- Screen 3: Manager Review -->
    <bpmn2:userTask id="UserTask_ManagerReview" name="Screen 3: Manager Review">
      <bpmn2:incoming>SequenceFlow_5</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_6</bpmn2:outgoing>
      <bpmn2:ioSpecification id="InputOutputSpecification_4">
        <bpmn2:dataInput id="DataInput_5" itemSubjectRef="_reportItem" name="report"/>
        <bpmn2:dataInput id="DataInput_6" name="TaskName"/>
        <bpmn2:dataOutput id="DataOutput_5" itemSubjectRef="_stringItem" name="managerComment"/>
        <bpmn2:dataOutput id="DataOutput_6" itemSubjectRef="_stringItem" name="managerDecision"/>
        <bpmn2:inputSet id="InputSet_4">
          <bpmn2:dataInputRefs>DataInput_5</bpmn2:dataInputRefs>
          <bpmn2:dataInputRefs>DataInput_6</bpmn2:dataInputRefs>
        </bpmn2:inputSet>
        <bpmn2:outputSet id="OutputSet_4">
          <bpmn2:dataOutputRefs>DataOutput_5</bpmn2:dataOutputRefs>
          <bpmn2:dataOutputRefs>DataOutput_6</bpmn2:dataOutputRefs>
        </bpmn2:outputSet>
      </bpmn2:ioSpecification>
      <bpmn2:dataInputAssociation id="DataInputAssociation_5">
        <bpmn2:sourceRef>report</bpmn2:sourceRef>
        <bpmn2:targetRef>DataInput_5</bpmn2:targetRef>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataInputAssociation id="DataInputAssociation_6">
        <bpmn2:targetRef>DataInput_6</bpmn2:targetRef>
        <bpmn2:assignment id="Assignment_3">
          <bpmn2:from xsi:type="bpmn2:tFormalExpression">Screen3_ManagerReview</bpmn2:from>
          <bpmn2:to xsi:type="bpmn2:tFormalExpression">DataInput_6</bpmn2:to>
        </bpmn2:assignment>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataOutputAssociation id="DataOutputAssociation_5">
        <bpmn2:sourceRef>DataOutput_5</bpmn2:sourceRef>
        <bpmn2:targetRef>managerComment</bpmn2:targetRef>
      </bpmn2:dataOutputAssociation>
      <bpmn2:dataOutputAssociation id="DataOutputAssociation_6">
        <bpmn2:sourceRef>DataOutput_6</bpmn2:sourceRef>
        <bpmn2:targetRef>managerDecision</bpmn2:targetRef>
      </bpmn2:dataOutputAssociation>
      <bpmn2:potentialOwner id="PotentialOwner_3">
        <bpmn2:resourceAssignmentExpression id="ResourceAssignmentExpression_3">
          <bpmn2:formalExpression>risk-manager</bpmn2:formalExpression>
        </bpmn2:resourceAssignmentExpression>
      </bpmn2:potentialOwner>
    </bpmn2:userTask>

    <!-- Gateway: Manager Decision -->
    <bpmn2:exclusiveGateway id="Gateway_ManagerDecision" name="Manager Decision?" gatewayDirection="Diverging">
      <bpmn2:incoming>SequenceFlow_6</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_Approve</bpmn2:outgoing>
      <bpmn2:outgoing>SequenceFlow_Return</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>

    <!-- Script: Update Status for Director -->
    <bpmn2:scriptTask id="ScriptTask_UpdateStatus2" name="Update Status" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>SequenceFlow_Approve</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_7</bpmn2:outgoing>
      <bpmn2:script><![CDATA[System.out.println("Status updated to SUBMITTED_TO_DIRECTOR");]]></bpmn2:script>
    </bpmn2:scriptTask>

    <!-- Screen 4: Director Approval -->
    <bpmn2:userTask id="UserTask_DirectorApproval" name="Screen 4: Director Approval">
      <bpmn2:incoming>SequenceFlow_7</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_8</bpmn2:outgoing>
      <bpmn2:ioSpecification id="InputOutputSpecification_5">
        <bpmn2:dataInput id="DataInput_7" itemSubjectRef="_reportItem" name="report"/>
        <bpmn2:dataInput id="DataInput_8" name="TaskName"/>
        <bpmn2:dataOutput id="DataOutput_7" itemSubjectRef="_stringItem" name="directorComment"/>
        <bpmn2:inputSet id="InputSet_5">
          <bpmn2:dataInputRefs>DataInput_7</bpmn2:dataInputRefs>
          <bpmn2:dataInputRefs>DataInput_8</bpmn2:dataInputRefs>
        </bpmn2:inputSet>
        <bpmn2:outputSet id="OutputSet_5">
          <bpmn2:dataOutputRefs>DataOutput_7</bpmn2:dataOutputRefs>
        </bpmn2:outputSet>
      </bpmn2:ioSpecification>
      <bpmn2:dataInputAssociation id="DataInputAssociation_7">
        <bpmn2:sourceRef>report</bpmn2:sourceRef>
        <bpmn2:targetRef>DataInput_7</bpmn2:targetRef>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataInputAssociation id="DataInputAssociation_8">
        <bpmn2:targetRef>DataInput_8</bpmn2:targetRef>
        <bpmn2:assignment id="Assignment_4">
          <bpmn2:from xsi:type="bpmn2:tFormalExpression">Screen4_DirectorApproval</bpmn2:from>
          <bpmn2:to xsi:type="bpmn2:tFormalExpression">DataInput_8</bpmn2:to>
        </bpmn2:assignment>
      </bpmn2:dataInputAssociation>
      <bpmn2:dataOutputAssociation id="DataOutputAssociation_7">
        <bpmn2:sourceRef>DataOutput_7</bpmn2:sourceRef>
        <bpmn2:targetRef>directorComment</bpmn2:targetRef>
      </bpmn2:dataOutputAssociation>
      <bpmn2:potentialOwner id="PotentialOwner_4">
        <bpmn2:resourceAssignmentExpression id="ResourceAssignmentExpression_4">
          <bpmn2:formalExpression>risk-director</bpmn2:formalExpression>
        </bpmn2:resourceAssignmentExpression>
      </bpmn2:potentialOwner>
    </bpmn2:userTask>

    <!-- Service Task: Archive -->
    <bpmn2:task id="Task_Archive" name="Archive Report" drools:taskName="ArchiveReport">
      <bpmn2:incoming>SequenceFlow_8</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_9</bpmn2:outgoing>
      <bpmn2:ioSpecification id="InputOutputSpecification_6">
        <bpmn2:dataInput id="DataInput_9" itemSubjectRef="_reportItem" name="report"/>
        <bpmn2:inputSet id="InputSet_6">
          <bpmn2:dataInputRefs>DataInput_9</bpmn2:dataInputRefs>
        </bpmn2:inputSet>
        <bpmn2:outputSet id="OutputSet_6"/>
      </bpmn2:ioSpecification>
      <bpmn2:dataInputAssociation id="DataInputAssociation_9">
        <bpmn2:sourceRef>report</bpmn2:sourceRef>
        <bpmn2:targetRef>DataInput_9</bpmn2:targetRef>
      </bpmn2:dataInputAssociation>
    </bpmn2:task>

    <!-- End Event -->
    <bpmn2:endEvent id="EndEvent_Success" name="Success End">
      <bpmn2:incoming>SequenceFlow_9</bpmn2:incoming>
    </bpmn2:endEvent>

    <!-- Sequence Flows -->
    <bpmn2:sequenceFlow id="SequenceFlow_1" sourceRef="StartEvent_1" targetRef="UserTask_PeriodSelection"/>
    <bpmn2:sequenceFlow id="SequenceFlow_2" sourceRef="UserTask_PeriodSelection" targetRef="Task_RetrieveData"/>
    <bpmn2:sequenceFlow id="SequenceFlow_3" sourceRef="Task_RetrieveData" targetRef="Gateway_DataCheck"/>

    <bpmn2:sequenceFlow id="SequenceFlow_DataOk" name="Yes" sourceRef="Gateway_DataCheck" targetRef="UserTask_IndicatorsDisplay">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression" language="http://www.java.com/java"><![CDATA[return success == true;]]></bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>

    <bpmn2:sequenceFlow id="SequenceFlow_DataError" name="No" sourceRef="Gateway_DataCheck" targetRef="EndEvent_Error">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression" language="http://www.java.com/java"><![CDATA[return success == false;]]></bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>

    <bpmn2:sequenceFlow id="SequenceFlow_4" sourceRef="UserTask_IndicatorsDisplay" targetRef="ScriptTask_UpdateStatus1"/>
    <bpmn2:sequenceFlow id="SequenceFlow_5" sourceRef="ScriptTask_UpdateStatus1" targetRef="UserTask_ManagerReview"/>
    <bpmn2:sequenceFlow id="SequenceFlow_6" sourceRef="UserTask_ManagerReview" targetRef="Gateway_ManagerDecision"/>

    <bpmn2:sequenceFlow id="SequenceFlow_Approve" name="Submit" sourceRef="Gateway_ManagerDecision" targetRef="ScriptTask_UpdateStatus2">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression" language="http://www.java.com/java"><![CDATA[return "SUBMIT".equals(managerDecision);]]></bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>

    <bpmn2:sequenceFlow id="SequenceFlow_Return" name="Return" sourceRef="Gateway_ManagerDecision" targetRef="UserTask_PeriodSelection">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression" language="http://www.java.com/java"><![CDATA[return "RETURN".equals(managerDecision);]]></bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>

    <bpmn2:sequenceFlow id="SequenceFlow_7" sourceRef="ScriptTask_UpdateStatus2" targetRef="UserTask_DirectorApproval"/>
    <bpmn2:sequenceFlow id="SequenceFlow_8" sourceRef="UserTask_DirectorApproval" targetRef="Task_Archive"/>
    <bpmn2:sequenceFlow id="SequenceFlow_9" sourceRef="Task_Archive" targetRef="EndEvent_Success"/>

  </bpmn2:process>

  <!-- Error Definition -->
  <bpmn2:error id="Error_1" errorCode="DATA_NOT_FOUND" name="Data Not Found"/>

  <!-- Diagram - Complete visual layout -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="LiquidityRiskIndicators">

      <!-- Start Event -->
      <bpmndi:BPMNShape id="BPMNShape_StartEvent_1" bpmnElement="StartEvent_1">
        <dc:Bounds x="100" y="200" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="106" y="243" width="25" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Screen 1: Period Selection -->
      <bpmndi:BPMNShape id="BPMNShape_UserTask_PeriodSelection" bpmnElement="UserTask_PeriodSelection">
        <dc:Bounds x="200" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Service Task: Retrieve Data -->
      <bpmndi:BPMNShape id="BPMNShape_Task_RetrieveData" bpmnElement="Task_RetrieveData">
        <dc:Bounds x="380" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Gateway: Data Check -->
      <bpmndi:BPMNShape id="BPMNShape_Gateway_DataCheck" bpmnElement="Gateway_DataCheck" isMarkerVisible="true">
        <dc:Bounds x="560" y="193" width="50" height="50"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="551" y="163" width="68" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Error End Event -->
      <bpmndi:BPMNShape id="BPMNShape_EndEvent_Error" bpmnElement="EndEvent_Error">
        <dc:Bounds x="567" y="330" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="556" y="373" width="50" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Screen 2: Indicators Display -->
      <bpmndi:BPMNShape id="BPMNShape_UserTask_IndicatorsDisplay" bpmnElement="UserTask_IndicatorsDisplay">
        <dc:Bounds x="680" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Script: Update Status 1 -->
      <bpmndi:BPMNShape id="BPMNShape_ScriptTask_UpdateStatus1" bpmnElement="ScriptTask_UpdateStatus1">
        <dc:Bounds x="860" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Screen 3: Manager Review -->
      <bpmndi:BPMNShape id="BPMNShape_UserTask_ManagerReview" bpmnElement="UserTask_ManagerReview">
        <dc:Bounds x="1040" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Gateway: Manager Decision -->
      <bpmndi:BPMNShape id="BPMNShape_Gateway_ManagerDecision" bpmnElement="Gateway_ManagerDecision" isMarkerVisible="true">
        <dc:Bounds x="1220" y="193" width="50" height="50"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1200" y="163" width="90" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Script: Update Status 2 -->
      <bpmndi:BPMNShape id="BPMNShape_ScriptTask_UpdateStatus2" bpmnElement="ScriptTask_UpdateStatus2">
        <dc:Bounds x="1330" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Screen 4: Director Approval -->
      <bpmndi:BPMNShape id="BPMNShape_UserTask_DirectorApproval" bpmnElement="UserTask_DirectorApproval">
        <dc:Bounds x="1510" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Service Task: Archive -->
      <bpmndi:BPMNShape id="BPMNShape_Task_Archive" bpmnElement="Task_Archive">
        <dc:Bounds x="1690" y="178" width="120" height="80"/>
      </bpmndi:BPMNShape>

      <!-- Success End Event -->
      <bpmndi:BPMNShape id="BPMNShape_EndEvent_Success" bpmnElement="EndEvent_Success">
        <dc:Bounds x="1870" y="200" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1853" y="243" width="62" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Sequence Flow: Start to Period Selection -->
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_1" bpmnElement="SequenceFlow_1">
        <di:waypoint x="136" y="218"/>
        <di:waypoint x="200" y="218"/>
      </bpmndi:BPMNEdge>

      <!-- Sequence Flow: Period Selection to Retrieve Data -->
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_2" bpmnElement="SequenceFlow_2">
        <di:waypoint x="320" y="218"/>
        <di:waypoint x="380" y="218"/>
      </bpmndi:BPMNEdge>

      <!-- Sequence Flow: Retrieve Data to Gateway -->
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_3" bpmnElement="SequenceFlow_3">
        <di:waypoint x="500" y="218"/>
        <di:waypoint x="560" y="218"/>
      </bpmndi:BPMNEdge>

      <!-- Sequence Flow: Gateway to Indicators Display (Data OK) -->
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_DataOk" bpmnElement="SequenceFlow_DataOk">
